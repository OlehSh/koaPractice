{
  "name": "koapractice",
  "version": "0.0.5",
  "description": "practice in koa and multi c services",
  "main": "index.js",
  "repository": "https://github.com/OlehSh/koaPractice",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc",
    "dev": "cross-env LOGLEVEL=debug tsnd --inspect -- ./src/index.ts",
    "neo4j:migrate:run": "neo4j-data-migrate app",
    "neo4j:migrate:rollback": "neo4j-data-migrate app ${index}",
    "neo4j:migrate:rollback:all": "neo4j-data-migrate app zero",
    "test:migrate": "NODE_ENV='test' neo4j-data-migrate app --database myapptest && NODE_ENV='test' neo4j-data-migrate test --database myapptest",
    "test:migrate:rollback": "NODE_ENV='test' neo4j-data-migrate app zero --database myapptest && NODE_ENV='test' neo4j-data-migrate test zero --database myapptest",
    "test": "npm run test:migrate; mocha --timeout 10000 --exit -r ts-node/register 'src/tests/**/**.test.ts'; npm run test:migrate:rollback",
    "test:coverage": "nyc npm run test",
    "docker:up": "docker-compose -f ./docker-compose.yaml up --build"
  },
  "author": "Oleg Shubin",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-joi-router": "^8.0.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.4",
    "koa-session": "^6.2.0",
    "neo4j-data-migrations": "^2.0",
    "neo4j-driver": "^4.3.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.0.0",
    "tsyringe": "^4.6.0",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/eslint": "^7.2.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/koa": "^2.13.3",
    "@types/koa-joi-router": "^8.0.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.12",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@typescript-eslint/types": "^4.28.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "src/tests",
      "src/index.ts",
      "src/registry.ts",
      "src/env.ts"
    ]
  }
}
