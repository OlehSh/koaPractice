{
  "name": "koapractice",
  "version": "0.0.5",
  "description": "practice in koa and multi c services",
  "main": "index.js",
  "repository": "https://github.com/OlehSh/koaPractice",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc",
    "dev": "cross-env LOGLEVEL=debug ts-node-dev --inspect -- ./src/index.ts",
    "neo4j:migrate:run": "neo4j-data-migrate app",
    "neo4j:migrate:rollback": "neo4j-data-migrate app ${Index}",
    "neo4j:migrate:rollback:all": "neo4j-data-migrate app zero",
    "test:migrate": "NODE_ENV='test' neo4j-data-migrate app --database myapptest",
    "test:migrate:rollback": "NODE_ENV='test' neo4j-data-migrate app zero --database myapptest",
    "test": "npm run test:migrate  && mocha --timeout 30000 --exit -r ts-node/register 'src/tests/**/**.test.ts' && npm run test:migrate:rollback",
    "test:coverage": "nyc npm run test",
    "docker:up": "docker-compose -f ./docker-compose.yaml up --build",
    "release": "release-it --no-npm"
  },
  "author": "Oleg Shubin",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^9.0.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-joi-router": "^8.0.0",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-session": "^6.1.0",
    "neo4j-data-migrations": "^2.0.3",
    "neo4j-driver": "^4.2.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.0",
    "tsyringe": "^4.5.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/eslint": "^7.2.10",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-joi-router": "^5.2.4",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/passport-jwt": "^3.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@typescript-eslint/types": "^4.15.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "release-it": "^14.6.2",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "src/tests",
      "src/index.ts",
      "src/registry.ts",
      "src/env.ts"
    ],
    "npm": {
      "publish": false
    },
    "release-it": {
      "github": {
        "release": true
      }
    }
  }
}
